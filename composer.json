{
    "name": "rianfuro/laravel-prometheus-storage-adapter",
    "description": "Laravel Eloquent storage adapter for the prometheus php client",
    "type": "library",
    "keywords": ["laravel", "eloquent", "prometheus", "storage", "adapter"],
    "require": {
        "laravel/framework": ">=6.0",
        "promphp/prometheus_client_php": "^2.3",
        "ext-json": "*"
    },
    "require-dev": {
        "orchestra/testbench": "*"
    },
    "autoload": {
        "psr-4": {
            "LaravelPrometheusStorageAdapter\\": "src/",
            "Tests\\": "tests/"
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Florian Proksch",
            "email": "florian.proksch@protonmail.com"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "LaravelPrometheusStorageAdapter\\ServiceProvider"
            ]
        }
    }
}
